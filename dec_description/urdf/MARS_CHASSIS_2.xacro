<!--MATERIAL COLORS -->
  <material name="blue">
    <color rgba="0 0 1 1"/>
  </material>

<material name="red">
    <color rgba="1 0 0 1"/>
  </material>


<material name="pink">
    <color rgba="0.8 0 1 0.5"/>
  </material>


    <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <material name="green">
    <color rgba="0 1 0 1"/>
</material>

<gazebo reference="base_link_to_wheel1">
  <material>Gazebo/orange</material>
</gazebo>

