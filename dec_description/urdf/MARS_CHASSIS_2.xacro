<!--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->

<!--Visual of the Link-->
  <link name="base_link">
    <visual>
    
    <geometry>
  
        <box size="0.5 0.40 0.40"/>
       </geometry>
  <!-- Setting up the origin of the link -->
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <material name="blue"/>
    </visual>
<!--  COLLISION -->
    <collision>
      <geometry>
       <box size="0.51 0.41 0.41"/>
       
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
<!--  INERTIAL -->
    <inertial>
      <mass value="5.0"/>
      
      <inertia
          ixx="0.1333333" ixy="0" ixz="0"
          iyy="0.1708333" iyz="0"
          izz="0.1708333"
      />
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </inertial>
  
  </link>
 
 
<!--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
<!--WHEEL_1--> 
<!--Visual of the Link-->
  <link name="wheel1">
    <visual>
      <geometry>
      <cylinder radius="${radius}" length="${length}"/>
         
      </geometry>
  <!-- Setting up the origin of the link -->
       <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/>
        <material name="white"/>
    </visual>
     <collision>
<!--  COLLISION -->
     <origin rpy="0 1.57075 1.57075" xyz="0 0 0"/>
      <geometry>
         <cylinder radius="${radius}" length="${length}"/>
      </geometry>
    </collision>
<!--  INERTIAL -->
 <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="0.4"/>
      <cylinder_inertia m="0.4" r="0.075" h="0.06"/>
      <inertia
        ixx="0.0006825" ixy="0.0" ixz="0.0"
        iyy="0.0006825" iyz="0.0"
        izz="0.001125"/>
    </inertial>

  </link>

<joint name="base_link_to_wheel1" type="continuous">
  <axis xyz="0 1 0"/> 
<origin rpy="0 0 0" xyz="0.125 0.23 -0.20"/>
 <limit effort="100" velocity="100"/>
 <joint_properties damping="0.0" friction="0.0"/>
    <parent link="base_link"/> 
    <child link="wheel1"/>
        
   
  </joint>


